<%= form_with model: @message, local: false, class: "" do |f| %>
  <%= f.text_field :name, class: "form-control", autocomplete: 'off' %>
  <%= f.text_field :content, class: "form-control", autocomplete: 'off' %>
  <%= f.text_field :sender_id, class: "form-control", autocomplete: 'off' %>
  <%= f.text_field :receiver_id, class: "form-control", autocomplete: 'off' %>
  <%= f.submit data: { disable_with: false } %>
<% end %>